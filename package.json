{
  "name": "@tinyhttp/jwt",
  "version": "1.3.1",
  "type": "module",
  "description": "tiny JWT middleware for Node.js",
  "homepage": "https://github.com/tinyhttp/jwt#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tinyhttp/jwt.git"
  },
  "engines": {
    "node": ">=12.4 || 14.x || >=16"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "keywords": [
    "tinyhttp",
    "nodejs",
    "web",
    "backend",
    "jwt"
  ],
  "author": "BRA1L0R",
  "contributors": [
    "BRA1L0R <https://brailor.me>",
    "v1rtl <https://v1rtl.site>"
  ],
  "license": "MIT",
  "dependencies": {
    "jsonwebtoken": "^9.0.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "node --experimental-loader esbuild-node-loader node_modules/uvu/bin.js tests",
    "test:coverage": "c8 --include=src pnpm test",
    "test:report": "c8 report --reporter=text-lcov > coverage.lcov",
    "lint": "eslint . --ext=ts",
    "format": "prettier --check \"./**/*.{ts,md}\"",
    "format:fix": "prettier --write \"./**/*.{ts,md}\"",
    "prepare": "pnpm build && pnpm lint && pnpm test"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "@tinyhttp/app": "1.3.11",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "c8": "^7.7.3",
    "esbuild-node-loader": "^0.1.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "expect": "^27.0.6",
    "prettier": "^2.3.2",
    "rollup": "^2.52.7",
    "supertest-fetch": "^1.4.3",
    "typescript": "^4.3.5",
    "uvu": "^0.5.1"
  }
}
